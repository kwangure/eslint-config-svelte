{
  "name": "@kwangure/eslint-config-svelte",
  "version": "0.1.13",
  "description": "An opinionated ESLint config for Svelte projects",
  "main": ".eslintrc.js",
  "scripts": {
    "release-candidate": "standard-version -a --prerelease rc && npm publish --tag rc && git push --follow-tags origin master",
    "release": "standard-version -a && npm publish && git push --follow-tags origin master"
  },
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-svelte3": "^3.2.0"
  },
  "devDependencies": {
    "standard-version": "^9.3.1"
  },
  "peerDependencies": {
    "eslint": "^7.19.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kwangure/eslint-config-svelte.git"
  },
  "keywords": [
    "eslint",
    "config",
    "eslintconfig",
    "svelte"
  ],
  "author": "Kafungo Wangure",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kwangure/eslint-config-svelte/issues"
  },
